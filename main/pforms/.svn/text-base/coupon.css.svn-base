body {
    margin: 3mm 5mm;
    font-size: 10px;
	
}

.div_move_right {
	margin-left: 0mm;	
}
.div_move_right_all {
	margin-left: 148mm;	
}
span {
    float: left;
    margin-left: 2mm;
    margin-bottom: 0.8mm;
    height: 4mm;
}
span table {
    margin-top: -1mm;
}
div {
    height: 3.5mm;
}
p {
    margin-top: 0.5mm;
    font-size: 12px;
}
.coupon_title {
    padding-bottom: 1mm;
    padding-left:  15mm;
    font-size: 14px;
}
.polis td, .passport td, .snils td, .ambul td, .birthday td, .obsluzh td, .work td, .gruppa td, .finans td, .date td {
    border: 1px solid black;
    width: 3.5mm;
    height: 3.5mm;
    font-weight: bold;
}
.sex div, .category div {
    float: left;
    width:  3mm;
    height: 3mm;
    border: 1px solid black;
    margin-left: 2mm;
    font-weight: bold;
}
.registr #code {
    margin-top: -2mm;
    padding: 0 4mm;
    min-width: 8mm;
    border-bottom: 1px solid black;
}
.registr #raion {
    margin-top: -2mm;
    padding: 0 4mm;
    min-width: 15mm;
    border-bottom: 1px solid black;
}
.registr #city {
    margin-top: -2mm;
    padding: 0 4mm;
    min-width: 10mm;
    border-bottom: 1px solid black;
}
.registr #street {
    margin-top: -2mm;
    padding: 0 2mm;
    min-width: 10mm;
    border-bottom: 1px solid black;
}
.registr #build {
    margin-top: -2mm;
    padding: 0 2mm;
    min-width: 5mm;
    border-bottom: 1px solid black;
}
.registr #appartment {
    margin-top: -2mm;
    padding: 0 2mm;
    min-width: 5mm;
    border-bottom: 1px solid black;
}
#ds, #smo, #fio, #kek, #work_place {
    float: left;
    margin-top: -1mm;
    width: 90mm;
    border-bottom: 1px solid black;
    font-weight: bold;
}
.obsluzh div {
    clear: both;
}
#obsluzh span {
    float : left;
    height: 17mm;
}
#work span {
    float : left;
    margin-top: -4mm;
    height: 10mm;
}
#title td {
    text-align: center;
}
.visit_spec td {
    border: 1px solid black;
    width: 3mm;
    height: 3mm;
    font-weight: bold;
}

table.cent td {
    text-align: center;
}